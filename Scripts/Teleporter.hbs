{{#test @root.E.Faction.Id ge 0}}
{{#teleporters @root.E.S '*'}}
{{#each .}}
{{#split CustomName '@'}}
{{#writefile @root.MainScriptPath (concat '..\\Teleporter\\Fac-' @root.E.Faction.Id '\\Data-' @root.E.Id '.txt')}}
{{.0}}@{{@root.E.Id}}:{{@root.P.Name}}
{{#if @root.P.IsPvP}}
{{@root.E.Name}} <color=purple>({{@root.E.EntityType}})</color> -> <color=red>{{@root.P.Name}}</color>
{{else}}
{{@root.E.Name}} <color=purple>({{@root.E.EntityType}})</color> -> <color=green>{{@root.P.Name}}</color>
{{/if}}
{{datetime}}
{{/writefile}}
{{#readfile @root.MainScriptPath (concat '..\\Teleporter\\Fac-' @root.E.Faction.Id '\\Data-' .1 '.txt')}}
{{#devices @root.E.S (concat 'Info:' CustomName '*')}}
{{#each .}}
{{#settextblock .}}
<align=center>{{../../../.1}}
<color=#d3d3d340><size=-3>{{datetime}} -> {{../../../.2}}</size></color></align>
{{/settextblock}}
{{/each}}
{{/devices}}
{{setteleporter @root.E.S ../../CustomName .0}}
{{else}}
{{#devices @root.E.S (concat 'Info:' CustomName '*')}}
{{#each .}}
{{#settextblock .}}
<align=center><size=10><sprite name="EntryForbidden_Bar"></size></align>
<align=center><color=#d3d3d340><size=2>{{datetime}}</size></color></align>
{{/settextblock}}
{{/each}}
{{/devices}}
{{/readfile}}
{{/split}}
{{/each}}
{{/teleporters}}
{{/test}}